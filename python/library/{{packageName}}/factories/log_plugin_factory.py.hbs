"""Log Plugin Factory."""

from {{packageName}}.plugins.logging import LogPlugin

from .factory import Factory


class LogPluginFactory(Factory[LogPlugin]):
    """Log Plugin Factory."""

    @property
    def _entry_points_registration_key(self) -> str:
        """Get entry points registration key."""
        return "{{packageName}}.plugins.logging"

    @property
    def _plugin_type(self):
        """Get the type of the plugin."""
        return LogPlugin
