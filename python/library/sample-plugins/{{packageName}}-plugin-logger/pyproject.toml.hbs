{{#equal buildTool "poetry"}}
[tool.poetry]
name = "{{packageName}}-plugin-logger"
version = "0.1.0"
description = ""
authors = ["{{{authoer}}}"]
readme = "README.md"
packages = [{ include = "{{packageName}}_plugin_logger" }]

[tool.poetry.dependencies]
python = "{{{pythonVersion}}}"


[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"

[tool.poetry.plugins."{{packageName}}.plugins.logging"]
ThirdPartyConsoleLogger = "{{packageName}}_plugin_logger.log_plugin_console:LogPluginConsole"
{{/equal}}
{{#equal buildTool "uv"}}
[project]
name = "{{packageName}}-plugin-logger"
version = "0.1.0"
description = "{{{description}}}"
readme = "README.md"
packages = [{ include = "{{packageName}}_plugin_logger" }]
requires-python = "{{{pythonVersion}}}"
dependencies = []

[build-system]
requires = ["hatchling"]
build-backend = "hatchling.build"

[project.entry-points."{{packageName}}.plugins.logging"]
ThirdPartyConsoleLogger = "{{packageName}}_plugin_logger.log_plugin_console:LogPluginConsole"
{{/equal}}